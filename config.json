{
	"cache": true,
	"duration": 200,
	"minify": true,
	"cacheByGet": true,
	"www": "www",
	"404": "www/404.html",
	"minifiers": {
		"html": {
			"mimes": ["text/html"],
			"extensions": ["html", "php", "html"]
		},
		"css": {
			"mimes": ["text/css"],
			"extensions": ["css"]
		},
		"javascript": {
			"mimes": ["text/javascript", "application/javascript"],
			"extensions": ["js"]
		},
		"jpeg": {
			"mimes": ["image/jpeg"],
			"extensions": ["jpg", "jpeg"]
		}
	},
	"index": ["index.php", "index.html"]
}